# Upper -cut off percent
upper-cut-off_percent=100

# remove stop words from the articles before comparison ?(yes/no)
use-stop_words=yes

# move duplicate files to a seperate folder(yes/no)
# applicable only if ip1_folder=ip2_folder
move_duplicate=no

# location for moving duplicate entries 
